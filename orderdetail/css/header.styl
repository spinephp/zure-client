.header
	width:100%
	height:48pt
	float:left
	color:white
	padding:12px 0 0 0
	background:#66aaee

.header p
	float:left
	font-weight:bold
	font-size:xx-large
	text-align:left
	margin:0px
	padding:16px 0px 16px 65px
	background:#66aaee url(../images/sb1.png) no-repeat 10px 50%

.header ol
	width:70px
	line-height:12px
	list-style:none
	padding:4px 0 0 0
	margin:0
	float:right

.header ol li
	padding:5px
	CURSOR: pointer
	font-size:x-small

.headers ul
	width:100%
	height:24px
	list-style:none
	padding:0
	margin:0px
	font-weight:bold
	font-size:small
	border:#00aaaa solid 1px
	border-left:0px
	border-right:0px
	clear:both
	background:url(../images/newsbg.gif) repeat-x left top

.headers ul li
	float:left
	padding:4px 10px

.headers ul li:last-child
	float:right
	padding:4px 10px

.headers ul li a
	text-decoration:none;

.footers
	width:100%
	text-align:center
	line-height:8px
	padding:1px 0
	margin:8px 0
	background:#6ae
	
.footers p
	font-size:xx-small
	
.footers p span
	margin-right:12px
