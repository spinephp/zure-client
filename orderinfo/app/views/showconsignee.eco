<dl>
  <dt>
    <b>收货人信息</b>
    <span>[<a class="edit">
      修改
    </a>]</span>
  </dt>
  <% if @name: %>
  <dd>
    <b><%= @name %></b> <span><%= @mobile %> <span><%= @tel %></span> <span><%= @email %></span>
    </dd>
  <dd>
    <lable><%= @getAddressFirst() %><%= @address %></lable>
    </dd>
    <% else: %>
  <dd>
    <lable>No Consignee</lable>
    </dd>
    <% end %>
  </dl>